package BTVNbuoi2;

import java.util.Scanner;

public class Bai1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner scan = new Scanner(System.in);
		int n = scan.nextInt();
		
		for (int i = 0 ; i < n ; i++ ){
			if (i == 0 || i == n - 1)
				for (int j = 0 ; j < n ; j++ )
					System.out.print("*");
			else {
				for (int j = 0; j < n; i++ )
					if (j == 0 || j == n - 1)
						System.out.print("*");
					else System.out.print(" ");
			}
		}
		
	}

}
